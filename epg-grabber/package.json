{
  "name": "epg",
  "version": "0.1.0",
  "scripts": {
    "update-codes": "node scripts/update-codes.js",
    "NpmInstall": "npm install --save",
    "grab1": "epg-grabber --config=./sites/mujtvprogram.cz.config.js --channels=./channels/r-mujtvprogram.xml --output=./guide/mujtvprogram.cz_cz.guide.xml --days=5",
    "grab2": "epg-grabber --config=./sites/horizon.tv.config.js --channels=./channels/r-horizon.xml --output=./guide/horizon.tv_sk.guide.xml --days=5",
