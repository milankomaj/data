    "grab4": "epg-grabber --config=./sites/***.js --channels=./channels/***.xml --output=./guide/***.guide.xml --days=5",
    "unit1": "epg-grabber --config=./sites/warner-tv.cz.config.js --channels=./channels/warner-tv.cz.chanell.xml --output=./guide/unit1.guide.xml --days=1",
